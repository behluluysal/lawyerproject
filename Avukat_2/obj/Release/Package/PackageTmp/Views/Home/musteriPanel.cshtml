<script>

    var modal = document.getElementById("myModal");
    var msid = "";
    var tipp = "";
    // Get the <span> element that closes the modal
    var span = document.getElementsByClassName("close")[0];
    span.onclick = function () {
        modal.style.display = "none";
        document.getElementById("dosyalar").innerHTML = "";
    }
    function openModal(item) {
        msid = item.id;
        var url = '@Url.Action("dosyalariGetir", "Scripts")';
        var datalar = msid.split('+');
        if (datalar[3] == "s") {   //Aktif Dava
            tipp = "aktif";
        }
        else if (datalar[3] == "e") {  //Biten Dava
            tipp = "biten";
        }

        var davaID = datalar[5];
        $.post(url, { davaid: davaID, tip: tipp }, function (data) {
            for (var i = 0; i < data.length - 1; i++) {
                icerik = "<button id=\"" + msid + "\" class=\"btn btn-default box-item\" onclick=\"downloadx(this)\">" + data[i] + "</button>";
                $('#dosyalar').append(icerik);
            }
        });
        modal.style.display = "block";

    };


</script>
<script>
    function downloadx(item) {
        var deneme = item.innerHTML;
        $('#gonderici').val(msid);
        $('#gondericiiki').val(deneme);
        $('#gondermeform').submit();
    }
</script>

