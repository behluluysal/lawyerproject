                    </div>
                    <!-- ---------MODAL --------------->
                    <div id="myModal" class="modal">
                        <!-- Modal content -->
                        <div class="modal-content text-center">
                            <span class="close">&times;</span>
                            <p>
                                <button type="button" class="btn btn-primary" onclick="bireyselForm()">BİREYSEL</button>
                                <button type="button" class="btn btn-primary" onclick="kurumsalForm()">KURUMSAL</button>
                            </p>
                            <div id="bireysel" style="display:block">
                                <div class="container-login100">
                                    <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-50">
                                        <form class="login100-form validate-form" method="post" action="/Home/Login">
                                            <div class="wrap-input100 validate-input">
                                                <input class="input100" type="text" id="tc" name="tc" placeholder="T.C. Kimlik No"  onkeypress="return event.charCode >= 48 && event.charCode <= 57" maxlength="11" required>
                                                <span class="focus-input100-1"></span>
                                                <span class="focus-input100-2"></span>
                                            </div>

                                            <div class="wrap-input100 rs1 validate-input">
                                                <input class="input100" type="password" id="tcsifre" name="tcsifre" placeholder="Şifre" required>
                                                <span class="focus-input100-1"></span>
                                                <span class="focus-input100-2"></span>
                                            </div>
                                            <div class="container-login100-form-btn m-t-20">
                                                <button type="submit" class="btn btn-primary login100-form-btn">
                                                    GİRİŞ YAP
                                                </button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <div id="kurumsal" style="display:none">
                                <div class="container-login100">
                                    <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-50">
                                        <form class="login100-form validate-form" method="post" action="/Home/Login">
                                            <div class="wrap-input100 validate-input">
                                                <input class="input100" type="text" id="vergi" name="tc" placeholder="Vergi No" onkeypress="return event.charCode >= 48 && event.charCode <= 57" maxlength="10" required >
                                                <span class="focus-input100-1"></span>
                                                <span class="focus-input100-2"></span>
                                            </div>

                                            <div class="wrap-input100 rs1 validate-input">
                                                <input class="input100" type="password" id="vergisifre" name="tcsifre" placeholder="Şifre" required>
                                                <span class="focus-input100-1"></span>
                                                <span class="focus-input100-2"></span>
                                            </div>
                                            <div class="container-login100-form-btn m-t-20">
                                                <button type="submit" class="btn btn-primary login100-form-btn">
                                                    GİRİŞ YAP
                                                </button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>

