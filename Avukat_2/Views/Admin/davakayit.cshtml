                     musteridurumu: deneme, musteriad: name, musteritc: tc, musteriemail: email, musteritel: tel, musterievfax: evtelno, musteriadres: adres, kayittarihi:kyttarihi,
                     /*devamı bireysel özellikleri*/ musteridt: dogumtarihi, musterisoyad: lname,
                     /*devamı dava bilgileri*/ davaadi: davaad, davadosya: ddosya, davaaciklama: daciklama, davaucret: ducret, davatarihi: dtarihi,
                     davano: dano, davatip: dtip, davaavukat: davukat/*musteri id en son ve gender*/, musteriid:mustid, cinsiyet:cins
                 }, function (data) {
                     if (data == "success") {
                         var myDropzone = Dropzone.forElement(".dropzone");
                         myDropzone.processQueue();
                         document.getElementById("dropzone1").submit();
                     }
                     else if (data == "error") {
                         PNotify.removeAll();
                         new PNotify({
                             title: 'Hata!',
                             text: 'Lütfen tekrar deneyiniz.',
                             type: 'error',
                             styling: 'bootstrap3'
                         });
                     }
                     else {
                         new PNotify({
                             title: 'Kritik Hata!',
                             text: 'Lütfen en kısa sürede teknik ekip ile iletişime geçiniz.',
                             type: 'error',
                             styling: 'bootstrap3'
                         });
                     }
                    
                 });

             }

        });
    });

