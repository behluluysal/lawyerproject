<script>
    var count = 0;
    var temp = "";
    var flagdz = true;
    var dzfilenames = "";
    Dropzone.autoDiscover = false;
    $("#dropzone1").dropzone({
        url: '/Admin/davakayit',
        maxFiles: 5,
        addRemoveLinks: true,
        maxfilesexceeded: function (file) {
            new PNotify({
                title: 'Uyarı',
                text: "Tek seferde en fazla 5 dosya yükleyebilirsiniz",
                type: 'notice',
                styling: 'bootstrap3'
            });
        },
        init: function () {
            var dz = this;
            this.on("addedfile", function (files) {

                var _i, _len;
                flagdz = true;
                for (_i = 0, _len = this.files.length; _i < _len - 1; _i++) // -1 to exclude current file
                {
                    if (this.files[_i].name === files.name) {
                        this.removeFile(files);
                        new PNotify({
                            title: 'Uyarı',
                            text: files.name + " Adlı dosya zaten işleme konulmuştur.",
                            type: 'notice',
                            styling: 'bootstrap3'
                        });
                        flagdz = false;
                    }
                }
                if (flagdz) {

                    dzfilenames += dz.files[count].name + "\\";
                    $("#dava-dosya").val(dzfilenames);
                    count++;
                }


                $('.dz-progress').hide();
            });
            this.on('sending', function (file, xhr, formData) {
                formData.append("test", $("#dava-musteri-id").val());
                formData.append("tip", kontroltip);
                formData.append("tc", $("#dava-musteri-no").val());
                formData.append("davaad", $("#dava-name").val());
            });
            this.on('removedfile', function (file, xhr, formData) {

                temp = $("#dava-dosya").val();
                temp = temp.replace(file.name + "\\", "");
                $("#dava-dosya").val(temp);
                dzfilenames = temp;
                temp = "";
                count--;
            });
        },

    });

</script>


